{
    "manifest_version": 2,
    "name": "Add YouTube Thumbnails",
    "description": "Adds YouTube thumbnails to RSS feed entries",
    "version": "0.0.1",
    "author": "fullmontis",
    "applications": {
        "gecko": {
            "id": "add-youtube-thumbnail@fullmontis.com",
            "strict_min_version": "78.0"
        }
    },
    "background": {
        "scripts": [
            "bg.js"
        ]
    },
    "permissions": [
        "messagesRead",
        "messagesModify"
    ]
}
